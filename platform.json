{
  "name": "avr128da",
  "title": "AVR 128DA MCU Platform",
  "description": "Copy paste from platformio/atmelmegaavr as a reference to learn how to modify-create platforms, in this case starting to replicate the AVR128DA characteristics",
  "homepage": "https://www.microchip.com/en-us/product/AVR128DA48",
  "license": "Apache-2.0",
  "keywords": [
    "dev-platform",
    "Microchip",
    "AVRDA",
    "AVR128",
    "8-bit"
  ],
  "engines": {
    "platformio": "^6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CoDePretzel/platform-avr128da.git"
  },
  "version": "1.6.0",
  "frameworks": {
    "arduino": {
      "package": "framework-arduino-megaavr",
      "script": "builder/frameworks/arduino.py"
    }
  },
  "packages": {
    "toolchain-atmelavr": {
      "type": "toolchain",
      "owner": "platformio",
      "version": "~1.70300.0",
      "optionalVersions": ["~3.70300.0"]
    },
    "framework-arduino-megaavr": {
      "type": "framework",
      "optional": true,
      "owner": "platformio",
      "version": "~1.8.7"
    },
    "framework-arduino-megaavr-megacorex": {
      "type": "framework",
      "optional": true,
      "owner": "platformio",
      "version": "~1.0.10"
    },
    "framework-arduino-megaavr-megatinycore": {
      "type": "framework",
      "optional": true,
      "owner": "platformio",
      "version": "~2.5.0"
    },
    "framework-arduino-megaavr-dxcore": {
      "type": "framework",
      "optional": true,
      "version": "~1.4.5"
    },
    "tool-avrdude-megaavr": {
      "type": "uploader",
      "optional": true,
      "owner": "platformio",
      "version": "~1.60300.0",
      "optionalVersions": ["~3.60300.0"]
    }
  }
}
